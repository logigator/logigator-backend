{
	"$schema": "http://json-schema.org/schema#",
	"$id": "/project/save",

	"type": "object",
	"properties": {
		"arguments": {
			"type": "object",
			"properties": {
				"id": {
					"type": "integer"
				}
			},
			"required": [ "id" ],
			"additionalProperties": false
		},
		"body": {
			"type": "object",
			"properties": {
				"data": {
					"type": "object",
					"properties": {
						"elements": {
							"type": "array",
							"items": {
								"type": "object",
								"properties": {
									"id": {
										"type": "integer"
									},
									"typeId": {
										"type": "integer"
									},
									"numOutputs": {
										"type": "integer"
									},
									"numInputs": {
										"type": "integer"
									},
									"pos": {
										"type": "object",
										"properties": {
											"x": {
												"type": "integer"
											},
											"y": {
												"type": "integer"
											}
										},
										"required": [ "x", "y" ],
										"additionalProperties": false
									},
									"endPos": {
										"type": "object",
										"properties": {
											"x": {
												"type": "integer"
											},
											"y": {
												"type": "integer"
											}
										},
										"required": [ "x", "y" ],
										"additionalProperties": false
									},
									"rotation": {
										"type": "integer"
									},
									"plugIndex": {
										"type": "integer"
									},
									"options": {
										"type": "array",
										"items": {
											"type": "number"
										}
									},
									"data": {
										"type": "string",
										"maxLength": 32768
									}
								},
								"required": [ "id", "typeId", "numOutputs", "numInputs", "pos", "rotation" ],
								"additionalProperties": false
							},
							"required": [ "elements" ],
							"additionalProperties": false
						},
						"mappings": {
							"type": "array",
							"items": {
								"type": "object",
								"properties": {
									"database": {
										"type": "integer"
									},
									"model": {
										"type": "integer"
									}
								},
								"required": [ "database", "model" ],
								"additionalProperties": false
							}
						}
					},
					"required": [ "elements", "mappings" ],
					"additionalProperties": false
				},
				"num_inputs": {
					"type": "integer"
				},
				"num_outputs": {
					"type": "integer"
				},
				"labels": {
					"type": "array",
					"maxLength": 512,
					"items": {
						"type": "string",
						"maxLength": 5,
						"pattern": "^[a-zA-Z0-9_\\-]*$"
					}
				}
			},
			"required": [ "data" ],
			"additionalProperties": false
		}
	},
	"required": [ "arguments", "body" ],
	"additionalProperties": false
}
